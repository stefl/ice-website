        "prop-types": "^15.6.2",
        "react": "^16.5.2",
        "react-dimensions": "^1.3.1",
        "react-dom": "^16.5.2",
        "react-emotion": "^9.2.12",
        "react-headroom": "^2.2.4",
        "react-helmet": "^5.2.0",
        "react-player": "^1.7.1",
        "react-scroll": "^1.7.10",
        "react-spring": "^6.1.10",
        "react-typography": "^0.16.13",
        "serve": "^10.1.1",
